@*partial view*@
@model IEnumerable<Case.ORM.RssPage>

<table class="table">
    <tr>
        <th>
            <label>Id</label>
        </th>
        <th>
            <label>URL</label>
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RssLink)
            </td>
            <th>
                @*Html.DisplayFor(modelItem => item.ContentXml)*@
            </th>
        </tr>
    }
</table>